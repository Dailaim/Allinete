{
	"name": "my-qwik-empty-starter",
	"description": "App with Routing built-in ready to create your app",
	"engines": {
		"node": ">=15.0.0"
	},
	"private": true,
	"trustedDependencies": ["sharp"],
	"scripts": {
		"build": "qwik build",
		"build-storybook": "storybook build",
		"build.client": "vite build",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.types": "tsc --incremental --noEmit",
		"deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
		"dev": "vite --mode ssr",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"lint": "eslint \"src/**/*.ts*\"",
		"preview": "qwik build preview && vite preview --open",
		"start": "vite --open --mode ssr",
		"storybook": "storybook dev -p 6006",
		"qwik": "qwik"
	},
	"devDependencies": {
		"@auth/core": "^0.13.0",
		"@biomejs/biome": "1.2.2",
		"@builder.io/qwik": "^1.2.17",
		"@builder.io/qwik-auth": "0.1.1",
		"@builder.io/qwik-city": "^1.2.17",
		"@storybook/addon-essentials": "7.5.2",
		"@storybook/addon-links": "7.5.2",
		"@storybook/blocks": "7.5.2",
		"@storybook/builder-vite": "7.5.2",
		"@storybook/html": "7.5.2",
		"@storybook/html-vite": "7.5.2",
		"@svgx/vite-plugin-qwik": "^1.0.1",
		"@tailwindcss/forms": "^0.5.7",
		"@types/eslint": "8.44.1",
		"@types/leaflet": "1.9.4",
		"@types/node": "^20.9.0",
		"@typescript-eslint/eslint-plugin": "6.2.0",
		"@typescript-eslint/parser": "6.2.0",
		"autoprefixer": "^10.4.16",
		"daisyui": "^3.9.4",
		"eslint": "8.45.0",
		"eslint-plugin-qwik": "^1.2.17",
		"eslint-plugin-storybook": "^0.6.15",
		"postcss": "8.4.27",
		"prettier": "3.0.0",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"storybook": "7.5.2",
		"storybook-framework-qwik": "0.2.4",
		"tailwindcss": "3.3.3",
		"typescript": "5.1.6",
		"undici": "5.22.1",
		"vite": "4.4.11",
		"vite-tsconfig-paths": "4.2.1"
	},
	"dependencies": {
		"@auth/surrealdb-adapter": "^0.1.3",
		"@modular-forms/qwik": "^0.21.0",
		"@motionone/dom": "^10.16.4",
		"@motionone/types": "^10.16.3",
		"@qwikest/icons": "^0.0.10",
		"leaflet": "1.9.4",
		"motion": "^10.16.4",
		"motion-qwik-adapter": "^10.16.9",
		"nodemailer": "^6.9.7",
		"surrealdb.js": "^0.9.1",
		"swiper": "^10.3.1",
		"valibot": "^0.19.0"
	}
}
